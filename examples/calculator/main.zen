func add(a: i32, b: i32) -> i32 {
    return a + b;
}

func multiply(x: i32, y: i32) -> i32 {
    return x * y;
}

func main() -> i32 {
    const first_number: i32 = 10;
    const second_number: i32 = 20;
    
    let sum = add(first_number, second_number);
    let product = multiply(sum, 2);
    
    print(sum);
    print(product);
    
    return 0;
}